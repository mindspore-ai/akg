def load_tensor(bin_path: str, expect_tensor: TensorType) -> TensorType:
    """从二进制文件加载MindSpore张量"""
    with open(bin_path, 'rb') as f:
        data = f.read()
        uint8_array = np.frombuffer(data, dtype=np.uint8)
        numpy_dtype = MS_TO_NP_DTYPE_MAP.get(expect_tensor.dtype)
